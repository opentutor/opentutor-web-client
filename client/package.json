{
  "name": "opentutor-web-client",
  "description": "web ui for interacting with opentutor",
  "version": "1.0.0",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@iostindex/gatsby-plugin-material-ui": "^5.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@xapi/cmi5": "^1.1.2",
    "axios": "^0.21.1",
    "gatsby": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.9.0",
    "react-scroll": "^1.8.2",
    "reading-time": "^1.5.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.0",
    "tss-react": "^4.8.2",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-scroll": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "gatsby-plugin-env-variables": "^2.3.0",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-manifest": "^5.5.0",
    "gatsby-plugin-sitemap": "^6.5.0",
    "gatsby-plugin-styled-components": "^6.9.0",
    "gatsby-plugin-typescript": "^5.5.0",
    "license-check-and-add": "^4.0.2",
    "npm-check-updates": "^11.6.0",
    "react-markdown": "^5.0.3",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "axios": "0.21.1"
  },
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "deps:update": "ncu -u",
    "test": "echo 'all tests currently cypress'",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx --max-warnings=0",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-web-client/issues"
  }
}
