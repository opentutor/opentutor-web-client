{
  "name": "opentutor-web-client",
  "description": "web ui for interacting with opentutor",
  "version": "1.0.0",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/material": "^5.8.1",
    "@reach/router": "^1.3.4",
    "@types/material-ui": "^0.21.12",
    "@xapi/cmi5": "^1.1.4",
    "axios": "^0.27.2",
    "gatsby": "^4.15.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-player": "^2.10.1",
    "react-scroll": "^1.8.7",
    "reading-time": "^1.5.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gatsby-cli": "^4.15.0",
    "gatsby-plugin-env-variables": "^2.2.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-material-ui": "^4.1.0",
    "gatsby-plugin-styled-components": "^5.15.0",
    "gatsby-plugin-typescript": "^4.15.0",
    "license-check-and-add": "^4.0.5",
    "npm-check-updates": "^13.0.3",
    "typescript": "^4.7.2"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "axios": "0.21.1"
  },
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "deps:update": "ncu -u",
    "test": "echo 'all tests currently cypress'",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx --max-warnings=0",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-web-client/issues"
  }
}
