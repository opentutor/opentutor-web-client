{
  "name": "opentutor-web-client",
  "description": "web ui for interacting with opentutor",
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "@xapi/cmi5": "^1.1.1",
    "axios": "^0.21.1",
    "gatsby": "^3.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scroll": "^1.8.2",
    "reading-time": "^1.3.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-scroll": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cli": "^3.7.1",
    "gatsby-plugin-env-variables": "^2.1.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-plugin-typescript": "^3.7.1",
    "license-check-and-add": "^4.0.2",
    "npm-check-updates": "^11.6.0",
    "typescript": "^4.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "axios": "0.21.1"
  },
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "deps:update": "ncu -u",
    "test": "echo 'all tests currently cypress'",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-web-client/issues"
  }
}
